<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StateButton">

        <!-- text color -->
        <attr name="normalTextColor" format="color|reference" />
        <attr name="pressedTextColor" format="color|reference" />
        <attr name="unableTextColor" format="color|reference" />

        <!-- stroke width and color, dash width, dash gap -->
        <attr name="strokeDashWidth" format="dimension|reference" />
        <attr name="strokeDashGap" format="dimension|reference" />
        <attr name="normalStrokeWidth" format="dimension|reference" />
        <attr name="pressedStrokeWidth" format="dimension|reference" />
        <attr name="unableStrokeWidth" format="dimension|reference" />
        <attr name="normalStrokeColor" format="color|reference" />
        <attr name="pressedStrokeColor" format="color|reference" />
        <attr name="unableStrokeColor" format="color|reference" />

        <!-- background color -->
        <attr name="normalBackgroundColor" format="color|reference" />
        <attr name="pressedBackgroundColor" format="color|reference" />
        <attr name="unableBackgroundColor" format="color|reference" />

        <!-- background radius -->
        <attr name="radius" format="dimension|reference" />
        <attr name="round" format="boolean|reference" />

        <!-- animation duration -->
        <attr name="animationDuration" format="integer|reference" />
    </declare-styleable>
    <declare-styleable name="customTextView">
        <attr name="textSolidColor" format="color" />
        <attr name="textSelectedSolidColor" format="color" />
        <attr name="textStrokeColor" format="color" />
        <attr name="textRadius" format="dimension" />
        <attr name="textLeftTopRadius" format="dimension" />
        <attr name="textLeftBottomRadius" format="dimension" />
        <attr name="textRightTopRadius" format="dimension" />
        <attr name="textRightBottomRadius" format="dimension" />
        <attr name="textStrokeWidth" format="dimension" />
        <attr name="textDrawable" format="reference" />
        <attr name="textNormalTextColor" format="color" />
        <attr name="textSelectedTextColor" format="color" />
    </declare-styleable>

    <!-- BaseBanner -->


    <!-- Banner高宽比,范围0-1,默认0.5(for LoopViewPager) -->
    <attr name="bb_scale" format="float" />

    <!-- for LoopViewPager start -->
    <!-- 是否支持循环,默认true -->
    <attr name="bb_isLoopEnable" format="boolean" />
    <!-- 滚动延时,默认5秒 -->
    <attr name="bb_delay" format="integer" />
    <!-- 滚动间隔,默认5秒 -->
    <attr name="bb_period" format="integer" />
    <!-- 是否支持自动滚动,默认true -->
    <attr name="bb_isAutoScrollEnable" format="boolean" />
    <!-- for LoopViewPager end -->


    <!-- 设置底部背景条颜色,默认透明 -->
    <attr name="bb_barColor" format="color" />
    <!-- 设置当滚动到最后一个item时是否显示底部背景条 -->
    <attr name="bb_isBarShowWhenLast" format="boolean" />
    <!-- 设置底部背景条padding,单位dp -->
    <attr name="bb_barPaddingLeft" format="dimension" />
    <attr name="bb_barPaddingTop" format="dimension" />
    <attr name="bb_barPaddingRight" format="dimension" />
    <attr name="bb_barPaddingBottom" format="dimension" />
    <!-- 设置标题文字颜色,默认"#ffffff" -->
    <attr name="bb_textColor" format="color" />
    <!-- 设置标题文字大小,单位sp,默认14sp -->
    <attr name="bb_textSize" format="dimension" />
    <!-- 设置是否显示标题,默认true -->
    <attr name="bb_isTitleShow" format="boolean" />
    <!-- 设置是否显示显示器,默认true -->
    <attr name="bb_isIndicatorShow" format="boolean" />
    <!-- 设置显示器位置.默认Gravity.CENTER.Gravity.CENTER时文字不显示 -->
    <attr name="bb_indicatorGravity">
        <enum name="CENTER" value="17" />
        <enum name="LEFT" value="3" />
        <enum name="RIGHT" value="5" />
    </attr>

    <!-- BaseIndicatorBanner -->
    <!-- 设置显示器样式 -->
    <attr name="bb_indicatorStyle">
        <enum name="DRAWABLE_RESOURCE" value="0" />
        <enum name="CORNER_RECTANGLE" value="1" />
    </attr>
    <!-- 设置显示器宽度,单位dp,默认6dp -->
    <attr name="bb_indicatorWidth" format="dimension" />
    <!-- 设置显示器高度,单位dp,默认6dp -->
    <attr name="bb_indicatorHeight" format="dimension" />
    <!-- 设置显示器间距,单位dp,默认6dp -->
    <attr name="bb_indicatorGap" format="dimension" />

    <!-- 设置显示器选中颜色,默认"#ffffff" -->
    <attr name="bb_indicatorSelectColor" format="color" />
    <!-- 设置显示器未选中颜色,默认"#88ffffff" -->
    <attr name="bb_indicatorUnselectColor" format="color" />
    <!-- 设置显示器圆角弧度,单位dp,默认3dp -->
    <attr name="bb_indicatorCornerRadius" format="dimension" />

    <!-- 设置显示器选中drawable资源 -->
    <attr name="bb_indicatorSelectRes" format="reference" />
    <!-- 设置显示器未选中drawable资源 -->
    <attr name="bb_indicatorUnselectRes" format="reference" />

    <declare-styleable name="BaseBanner">
        <attr name="bb_scale" />
        <attr name="bb_isLoopEnable" />
        <attr name="bb_delay" />
        <attr name="bb_period" />
        <attr name="bb_isAutoScrollEnable" />
        <attr name="bb_barColor" />
        <attr name="bb_isBarShowWhenLast" />
        <attr name="bb_barPaddingLeft" />
        <attr name="bb_barPaddingTop" />
        <attr name="bb_barPaddingRight" />
        <attr name="bb_barPaddingBottom" />
        <attr name="bb_textColor" />
        <attr name="bb_textSize" />
        <attr name="bb_isTitleShow" />
        <attr name="bb_isIndicatorShow" />
        <attr name="bb_indicatorGravity" />
    </declare-styleable>
    <declare-styleable name="BaseIndicatorBanner">
        <attr name="bb_indicatorStyle" />
        <attr name="bb_indicatorWidth" />
        <attr name="bb_indicatorHeight" />
        <attr name="bb_indicatorGap" />
        <attr name="bb_indicatorSelectColor" />
        <attr name="bb_indicatorUnselectColor" />
        <attr name="bb_indicatorCornerRadius" />
        <attr name="bb_indicatorSelectRes" />
        <attr name="bb_indicatorUnselectRes" />
    </declare-styleable>
    <declare-styleable name="JPTabBar">
        <attr name="TabHeight" format="reference|dimension" />
        <attr name="TabNormalColor" format="reference|color" />
        <attr name="TabSelectColor" format="reference|color" />
        <attr name="TabSelectBg" format="reference|color" />
        <attr name="TabTextSize" format="dimension|reference" />
        <attr name="TabIconSize" format="dimension|reference" />
        <attr name="TabIconFilter" format="boolean" />
        <attr name="TabMargin" format="dimension|reference" />
        <attr name="TabAnimate" format="enum">
            <enum name="Flip" value="1" />
            <enum name="Rotate" value="2" />
            <enum name="Scale" value="3" />
            <enum name="Jump" value="4" />
            <enum name="None" value="5" />
        </attr>
        <attr name="TabMiddleIcon" format="reference" />
        <attr name="BadgeColor" format="reference|color" />
        <attr name="BadgeDraggable" format="boolean" />
        <attr name="BadgePadding" format="dimension|reference" />
        <attr name="BadgeTextSize" format="reference|dimension" />
        <attr name="BadgeVerticalMargin" format="reference|dimension" />
        <attr name="BadgeHorizonalMargin" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="FabOptions">
        <attr name="button_menu" format="reference" type="menu" />
    </declare-styleable>

    <!-- PagerSlidingTab -->
    <declare-styleable name="PagerSlidingTab">
        <attr name="indicatorColor" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="pst_dividerPadding" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tab_Background" format="reference" />
        <attr name="shouldExpand" format="boolean" />
        <attr name="pst_textAllCaps" format="boolean" />
    </declare-styleable>
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0" />
            <enum name="y" value="1" />
            <enum name="z" value="2" />
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction" />
        <attr name="pivotY" format="fraction" />
    </declare-styleable>
    <declare-styleable name="LabelView">
        <attr name="backgroundColor" format="color" />
        <attr name="textTitleColor" format="color" />
        <attr name="textContentColor" format="color" />
        <attr name="textTitle" format="string" />
        <attr name="textContent" format="string" />
        <attr name="textTitleSize" format="dimension" />
        <attr name="textContentSize" format="dimension" />
        <attr name="labelTopPadding" format="dimension" />
        <attr name="labelCenterPadding" format="dimension" />
        <attr name="labelBottomPadding" format="dimension" />
        <attr name="labelTopDistance" format="dimension" />
        <attr name="textTitleStyle">
            <enum name="normal" value="0" />
            <enum name="italic" value="1" />
            <enum name="bold" value="2" />
        </attr>
        <attr name="textContentStyle">
            <enum name="normal" value="0" />
            <enum name="italic" value="1" />
            <enum name="bold" value="2" />
        </attr>
        <attr name="direction">
            <enum name="leftTop" value="-45" />
            <enum name="rightTop" value="45" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MarqueeView">
        <attr name="marqueeInterval" format="integer" />
        <attr name="marqueeAnimDuration" format="integer" />
        <attr name="marqueeAnimIn" format="reference" />
        <attr name="marqueeAnimOut" format="reference" />
    </declare-styleable>

    <attr name="styleLoadingLayout" format="reference" />

    <declare-styleable name="LoadingLayout">

        <!-- 空布局资源id -->
        <attr name="llEmptyResId" format="reference" />
        <!-- 加载中布局资源id -->
        <attr name="llLoadingResId" format="reference" />
        <!-- 错误布局资源id -->
        <attr name="llErrorResId" format="reference" />

        <!-- 空布局图片 -->
        <attr name="llEmptyImage" format="reference" />
        <!-- 空布局文本 -->
        <attr name="llEmptyText" format="string" />

        <!-- 错误布局图片 -->
        <attr name="llErrorImage" format="reference" />
        <!-- 错误布局文本 -->
        <attr name="llErrorText" format="string" />
        <!-- 错误布局重试按钮文本 -->
        <attr name="llRetryText" format="string" />

        <!-- 文本颜色 -->
        <attr name="llTextColor" format="color" />
        <!-- 文本尺寸 -->
        <attr name="llTextSize" format="dimension" />

        <!-- 按钮文本颜色 -->
        <attr name="llButtonTextColor" format="color" />
        <!-- 按钮文本尺寸 -->
        <attr name="llButtonTextSize" format="dimension" />
        <!-- 按钮背景 -->
        <attr name="llButtonBackground" format="reference" />
    </declare-styleable>

    <!-- SwitchIconView -->
    <declare-styleable name="SwitchIconView">
        <attr name="si_tint_color" format="color" />
        <attr name="si_animation_duration" format="integer" />
        <attr name="si_disabled_alpha" format="float" />
        <attr name="si_disabled_color" format="color" />
        <attr name="si_enabled" format="boolean" />
        <attr name="si_no_dash" format="boolean" />
    </declare-styleable>

    <!-- PathView -->
    <declare-styleable name="PathView">
        <attr name="pathColor" format="color" />
        <attr name="pathWidth" format="float" />
        <attr name="svg" format="reference" />
    </declare-styleable>

    <!-- imagepicker -->
    <declare-styleable name="CropImageView">
        <attr name="cropMaskColor" format="color" />
        <attr name="cropBorderColor" format="color" />
        <attr name="cropBorderWidth" format="dimension" />
        <attr name="cropFocusWidth" format="dimension" />
        <attr name="cropFocusHeight" format="dimension" />
        <attr name="cropStyle" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>
    <!-- TabView -->
    <declare-styleable name="TabView">
        <attr name="tab_textViewSelColor" format="reference|color" />
        <attr name="tab_textViewUnSelColor" format="reference|color" />
        <attr name="tab_tabViewBackgroundColor" format="reference|color" />
        <attr name="tab_tabViewHeight" format="dimension" />
        <attr name="imageViewTextViewMargin" format="dimension" />
        <attr name="tab_textViewSize" format="dimension" />
        <attr name="tab_imageViewWidth" format="dimension" />
        <attr name="tab_imageViewHeight" format="dimension" />
        <attr name="tab_tabViewGravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
        </attr>
        <attr name="tab_tabViewDefaultPosition" format="integer" />
        <attr name="tab_isNeedSplitLine" format="boolean" />
        <attr name="tab_splitLineColor" format="reference|color" />
    </declare-styleable>

    <!-- SmoothProgressBar -->
    <declare-styleable name="SmoothProgressBar">
        <attr name="spbStyle" format="reference" />
        <attr name="spb_color" format="color" />
        <attr name="spb_stroke_width" format="dimension" />
        <attr name="spb_stroke_separator_length" format="dimension" />
        <attr name="spb_sections_count" format="integer" />
        <attr name="spb_speed" format="float" />
        <attr name="spb_progressiveStart_speed" format="float" />
        <attr name="spb_progressiveStop_speed" format="float" />
        <attr name="spb_interpolator" format="enum">
            <enum name="spb_interpolator_accelerate" value="0" />
            <enum name="spb_interpolator_linear" value="1" />
            <enum name="spb_interpolator_acceleratedecelerate" value="2" />
            <enum name="spb_interpolator_decelerate" value="3" />
        </attr>
        <attr name="spb_reversed" format="boolean" />
        <attr name="spb_mirror_mode" format="boolean" />
        <attr name="spb_colors" format="reference" />
        <attr name="spb_progressiveStart_activated" format="boolean" />
        <attr name="spb_background" format="reference" />
        <attr name="spb_generate_background_with_colors" format="boolean" />
        <attr name="spb_gradients" format="boolean" />
    </declare-styleable>

    <!-- CircularProgressBar -->
    <declare-styleable name="CircularProgressBar">
        <attr name="cpbStyle" format="reference" />
        <attr name="cpb_color" format="color" />
        <attr name="cpb_colors" format="reference" />
        <attr name="cpb_stroke_width" format="dimension" />
        <attr name="cpb_min_sweep_angle" format="integer" />
        <attr name="cpb_max_sweep_angle" format="integer" />
        <attr name="cpb_sweep_speed" format="float" />
        <attr name="cpb_rotation_speed" format="float" />
    </declare-styleable>

    <!-- signatureView -->
    <declare-styleable name="signatureView">
        <attr name="PenSize" format="dimension" />
        <attr name="PenColor" format="color" />
        <attr name="BackColor" format="color" />
    </declare-styleable>

    <!-- pickerview -->
    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="pickerview_textSize" format="dimension" />
        <attr name="pickerview_textColorOut" format="color" />
        <attr name="pickerview_textColorCenter" format="color" />
        <attr name="pickerview_dividerColor" format="color" />
        <attr name="pickerview_lineSpacingMultiplier" format="float" />
    </declare-styleable>

    <!-- FlowLayout -->
    <declare-styleable name="FlowLayout">
        <attr name="horizontal_spacing" format="dimension" />
        <attr name="vertical_spacing" format="dimension" />
    </declare-styleable>

    <!-- TimeLineView -->
    <declare-styleable name="TimeLineView">
        <attr name="TLV_startedLineColor" format="color" />
        <attr name="TLV_preLineColor" format="color" />
        <attr name="TLV_startedCircleColor" format="color" />
        <attr name="TLV_underwayCircleColor" format="color" />
        <attr name="TLV_preCircleColor" format="color" />
        <attr name="TLV_startedStringColor" format="color" />
        <attr name="TLV_underwayStringColor" format="color" />
        <attr name="TLV_preStringColor" format="color" />
        <attr name="TLV_textSize" format="dimension" />
        <attr name="TLV_radius" format="dimension" />
        <attr name="TLV_lineWidth" format="dimension" />
    </declare-styleable>
    <!-- FlowView -->
    <declare-styleable name="FlowViewHorizontal">
        <attr name="h_bg_radius" format="dimension" />
        <attr name="h_pro_radius" format="dimension" />
        <attr name="h_bg_width" format="dimension" />
        <attr name="h_bg_color" format="color" />
        <attr name="h_pro_width" format="dimension" />
        <attr name="h_pro_color" format="color" />
        <attr name="h_text_padding" format="dimension" />
        <attr name="h_time_padding" format="dimension" />
        <attr name="h_max_step" format="integer" />
        <attr name="h_textsize" format="dimension" />
        <attr name="h_pro_step" format="integer" />
    </declare-styleable>
    <declare-styleable name="FlowViewVertical">
        <attr name="v_bg_radius" format="dimension" />
        <attr name="v_pro_radius" format="dimension" />
        <attr name="v_bg_width" format="dimension" />
        <attr name="v_bg_color" format="color" />
        <attr name="v_pro_width" format="dimension" />
        <attr name="v_pro_color" format="color" />
        <attr name="v_interval" format="dimension" />
        <attr name="v_bgPositionX" format="dimension" />
        <attr name="v_max_step" format="integer" />
        <attr name="v_pro_step" format="integer" />
        <attr name="v_textPaddingLeft" format="dimension" />
        <attr name="v_timePaddingRight" format="dimension" />
        <attr name="v_textMoveTop" format="dimension" />
        <attr name="v_timeMoveTop" format="dimension" />
        <attr name="v_textsize" format="dimension" />
    </declare-styleable>
    
    <!-- Alerter -->
    <declare-styleable name="Alerter">
        <attr name="alertStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="Alert">
        <attr name="alertIcon" format="reference" />
        <attr name="alertBackgroundColor" format="color" />
        <attr name="alertTitle" format="string" />
        <attr name="alertText" format="string" />
    </declare-styleable>

</resources>